{
  "name": "@ayingott/prettier-config",
  "version": "0.0.0",
  "description": "prettier-config",
  "type": "module",
  "packageManager": "pnpm@8.7.0",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/prettier-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/prettier-config/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.4.0",
    "@types/node": "^20.5.7",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.0",
    "prettier": "^3.0.2",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}
